        "@babel/plugin-transform-async-to-generator": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "browser-sync": "^2.26.13",
        "browser-sync-webpack-plugin": "^2.3.0",
