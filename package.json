{
    "name": "phaser-3-generic-platformer",
    "version": "0.0.1-alpha.2",
    "description": "Description here.",
    "author": "Pablo Pirata",
    "main": "index.js",
    "scripts": {
        "dev": "npm install && webpack",
        "start": "webpack",
        "build": "webpack --config webpack.production.config.js",
        "prepare-mobile": "cd mobile && cordova platform add android && cd ..",
        "build-mobile": "webpack --config webpack.production.config.js --env=mobile",
        "build-apk": "cd mobile && cordova build --release android && cd ..",
        "deploy": "npm run build-mobile && npm run build-apk",
        "test": "jest",
        "bump": "ncu -u -t minor"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.10",
